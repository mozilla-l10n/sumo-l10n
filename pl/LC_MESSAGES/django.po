#: kitsune/sumo/db_strings.py:168 kitsune/sumo/db_strings.py:308 kitsune/sumo/db_strings.py:546
msgctxt "DB: products.Topic.title"
msgid "Learn the Basics: get started"
msgstr "Poznaj podstawy — zaczynamy"

#: kitsune/sumo/db_strings.py:169
msgctxt "DB: products.Topic.description"
msgid "Learn all you need to know to get started with Firefox."
msgstr "Wszystko, co potrzebujesz, aby rozpocząć pracę z Firefoksem."

#: kitsune/sumo/db_strings.py:170 kitsune/sumo/db_strings.py:364
msgctxt "DB: products.Topic.title"
msgid "Bookmarks and tabs"
msgstr "Zakładki i karty"

#: kitsune/sumo/db_strings.py:171
msgctxt "DB: products.Topic.description"
msgid "Access and organize your favorite webpages easily with bookmarks and tabs"
msgstr "Organizuj i otwieraj twoje ulubione strony za pomocą zakładek oraz kart"

#: kitsune/sumo/db_strings.py:172 kitsune/sumo/db_strings.py:282 kitsune/sumo/db_strings.py:354
msgctxt "DB: products.Topic.title"
msgid "Basic browsing"
msgstr "Przeglądanie stron"

#: kitsune/sumo/db_strings.py:173 kitsune/sumo/db_strings.py:283
msgctxt "DB: products.Topic.description"
msgid "Search and navigate easily with these essential features"
msgstr "W prosty sposób wyszukuj i nawiguj"

#: kitsune/sumo/db_strings.py:174
msgctxt "DB: products.Topic.title"
msgid "Import settings from other browsers"
msgstr "Import ustawień z innych przeglądarek"

#: kitsune/sumo/db_strings.py:175
msgctxt "DB: products.Topic.description"
msgid "Learn how to import or export your information between Firefox and another browser"
msgstr "Dowiedz się, jak importować i eksportować dane pomiędzy Firefoksem a innymi przeglądarkami"

#: kitsune/sumo/db_strings.py:176
msgctxt "DB: products.Topic.title"
msgid "Video, audio and interactive settings"
msgstr "Ustawienia wideo, audio i interakcji"

#: kitsune/sumo/db_strings.py:177
msgctxt "DB: products.Topic.description"
msgid "Change how Firefox handles videos, animations, music and other interactive content"
msgstr "Zmień sposób, w jaki Firefox obsługuje wideo, animacje, muzykę oraz inną interaktywną zawartość"

#: kitsune/sumo/db_strings.py:178
msgctxt "DB: products.Topic.title"
msgid "How to use Firefox"
msgstr "Podstawy Firefoksa"

#: kitsune/sumo/db_strings.py:179
msgctxt "DB: products.Topic.description"
msgid "How to browse, search and customize your settings"
msgstr "Przeglądanie, wyszukiwanie i personalizowanie ustawień"

#: kitsune/sumo/db_strings.py:180 kitsune/sumo/db_strings.py:310 kitsune/sumo/db_strings.py:506
msgctxt "DB: products.Topic.title"
msgid "Download, install and migration"
msgstr "Pobieranie, instalacja i migracja"

#: kitsune/sumo/db_strings.py:181
msgctxt "DB: products.Topic.description"
msgid "Learn how to download Firefox on your desktop devices or move information to and from other browsers."
msgstr "Dowiedz się, jak pobrać Firefoksa na urządzenia stacjonarne oraz jak przenieść dane z i do innych przeglądarek."

#: kitsune/sumo/db_strings.py:182 kitsune/sumo/db_strings.py:312 kitsune/sumo/db_strings.py:466
msgctxt "DB: products.Topic.title"
msgid "Tips and tricks"
msgstr "Porady i wskazówki"

#: kitsune/sumo/db_strings.py:183
msgctxt "DB: products.Topic.description"
msgid "Go beyond the basics with these shortcuts and other tips."
msgstr "Podstawy to za mało? Poznaj skróty klawiszowe i inne porady."

#: kitsune/sumo/db_strings.py:184 kitsune/sumo/db_strings.py:188 kitsune/sumo/db_strings.py:288
msgctxt "DB: products.Topic.title"
msgid "Install and update"
msgstr "Instalacja i aktualizacja"

#: kitsune/sumo/db_strings.py:185
msgctxt "DB: products.Topic.description"
msgid "How to install Firefox and keep it up to date"
msgstr "Jak zainstalować i aktualizować Firefoksa"

#: kitsune/sumo/db_strings.py:186
msgctxt "DB: products.Topic.title"
msgid "Display and appearance"
msgstr "Wyświetlanie i wygląd"

#: kitsune/sumo/db_strings.py:187
msgctxt "DB: products.Topic.description"
msgid "Learn how to change your toolbar, font sizes and browser colors"
msgstr "Dowiedz się, jak zmienić pasek narzędzi, rozmiar czcionek i kolory przeglądarki"

#: kitsune/sumo/db_strings.py:189
msgctxt "DB: products.Topic.description"
msgid "Download or update Firefox for Windows, Mac and Linux."
msgstr "Pobieranie i aktualizacja Firefoksa dla Windows, MacOS i Linuksa."

#: kitsune/sumo/db_strings.py:190
msgctxt "DB: products.Topic.title"
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: kitsune/sumo/db_strings.py:191 kitsune/sumo/db_strings.py:317
msgctxt "DB: products.Topic.description"
msgid "Firefox Sync settings"
msgstr "Ustawienia Firefox Sync"

#: kitsune/sumo/db_strings.py:192
msgctxt "DB: products.Topic.title"
msgid "Sync and save"
msgstr "Synchronizuj i zapisz"

#: kitsune/sumo/db_strings.py:193
msgctxt "DB: products.Topic.description"
msgid "Sync information on all your devices"
msgstr "Zsynchronizuj swoje dane na wszystkich urządzeniach"

#: kitsune/sumo/db_strings.py:194
msgctxt "DB: products.Topic.title"
msgid "Manage add-ons"
msgstr "Zarządzanie dodatkami"

#: kitsune/sumo/db_strings.py:195
msgctxt "DB: products.Topic.description"
msgid "Enhance Firefox's functionality and appearance with add-ons"
msgstr "Wzbogać Firefoksa o nowe możliwości i funkcje"

#: kitsune/sumo/db_strings.py:196
msgctxt "DB: products.Topic.title"
msgid "Sync, share and save"
msgstr "Zapisywanie, udostępnianie i synchronizacja"

#: kitsune/sumo/db_strings.py:197
msgctxt "DB: products.Topic.description"
msgid "Sync browsing information and content across multiple devices with Firefox Accounts."
msgstr "Synchronizacja danych przeglądania i zawartości pomiędzy urządzeniami za pomocą Konta Firefoksa."

#: kitsune/sumo/db_strings.py:198
msgctxt "DB: products.Topic.title"
msgid "Firefox settings"
msgstr "Ustawienia Firefox Sync"

#: kitsune/sumo/db_strings.py:199
msgctxt "DB: products.Topic.description"
msgid "Privacy and personalization"
msgstr "Prywatność i bezpieczeństwo"

#: kitsune/sumo/db_strings.py:200
msgctxt "DB: products.Topic.title"
msgid "Firefox Hello"
msgstr "Firefox Hello"

#: kitsune/sumo/db_strings.py:201
msgctxt "DB: products.Topic.description"
msgid "Have video or voice conversations using the Firefox browser"
msgstr "Prowadź rozmowy i wideorozmowy bezpośrednio w Firefoksie"

#: kitsune/sumo/db_strings.py:202
msgctxt "DB: products.Topic.title"
msgid "Chat and share"
msgstr "Rozmowa i udostępnianie"

#: kitsune/sumo/db_strings.py:203
msgctxt "DB: products.Topic.description"
msgid "Connect on video and share pages with your network"
msgstr "Prowadź wideokonferencje i udostępniaj strony swoim znajomym"

#: kitsune/sumo/db_strings.py:204 kitsune/sumo/db_strings.py:208
msgctxt "DB: products.Topic.title"
msgid "Personalize Firefox"
msgstr "Personalizacja Firefoksa"

#: kitsune/sumo/db_strings.py:205
msgctxt "DB: products.Topic.description"
msgid "Make Firefox yours with these customization options."
msgstr "Dopasuj Firefoksa do własnego stylu pracy."

#: kitsune/sumo/db_strings.py:206 kitsune/sumo/db_strings.py:322 kitsune/sumo/db_strings.py:494
msgctxt "DB: products.Topic.title"
msgid "Customize controls, options and add-ons"
msgstr "Elementy sterujące, funkcje i dodatki"

#: kitsune/sumo/db_strings.py:207
msgctxt "DB: products.Topic.description"
msgid "Make Firefox yours by adding and managing the features that you want."
msgstr "Dopasuj Firefoksa do siebie dodając i zarządzając funkcjami, których potrzebujesz."

#: kitsune/sumo/db_strings.py:209
msgctxt "DB: products.Topic.description"
msgid "Change Firefox's appearance, behavior and settings."
msgstr "Zmień wygląd i ustawienia Firefoksa."

#: kitsune/sumo/db_strings.py:210 kitsune/sumo/db_strings.py:320 kitsune/sumo/db_strings.py:516
msgctxt "DB: products.Topic.title"
msgid "Privacy and security settings"
msgstr "Ustawienia prywatności i bezpieczeństwa"

#: kitsune/sumo/db_strings.py:211
msgctxt "DB: products.Topic.description"
msgid "Learn how to keep your information safe and secure with Firefox's private browsing, password features and other security settings."
msgstr "Dowiedz się, jak uchronić swoje dane wykorzystując tryb prywatny Firefoksa, ochronę hasłem oraz inne ustawienia zabezpieczeń."

#: kitsune/sumo/db_strings.py:212 kitsune/sumo/db_strings.py:302
msgctxt "DB: products.Topic.title"
msgid "Do more with apps"
msgstr "Dodatkowe aplikacje"

#: kitsune/sumo/db_strings.py:213
msgctxt "DB: products.Topic.description"
msgid "Install open apps from the Marketplace to add more fun and functionality to your device"
msgstr "Zainstaluj bezpłatne aplikacje ze sklepu Marketplace i zwiększ funkcjonalność swojego urządzenia"

#: kitsune/sumo/db_strings.py:214 kitsune/sumo/db_strings.py:298
msgctxt "DB: products.Topic.title"
msgid "Protect your privacy"
msgstr "Ochrona prywatności"

#: kitsune/sumo/db_strings.py:215
msgctxt "DB: products.Topic.description"
